{
  "name": "@midcurve/api",
  "version": "0.1.0",
  "description": "RESTful API for Midcurve Finance - concentrated liquidity position management",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "api:request": "tsx scripts/api-request.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "npm run test",
    "test:e2e:run": "npm run test:run",
    "postinstall": "npm run prisma:sync && prisma generate",
    "prisma:sync": "bash scripts/sync-prisma-schema.sh",
    "prisma:generate": "npm run prisma:sync && prisma generate",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:test:up": "docker-compose -f docker-compose.test.yml up -d",
    "db:test:down": "docker-compose -f docker-compose.test.yml down",
    "db:test:reset": "docker-compose -f docker-compose.test.yml down -v && docker-compose -f docker-compose.test.yml up -d",
    "yalc:link:services": "yalc add @midcurve/services && npm install",
    "yalc:link:shared": "yalc add @midcurve/shared && npm install",
    "yalc:link:all": "yalc add @midcurve/services @midcurve/shared && npm install",
    "yalc:update": "yalc update",
    "yalc:unlink": "yalc remove @midcurve/services @midcurve/shared && npm install",
    "yalc:clean": "rimraf .yalc yalc.lock"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "midcurve",
    "defi",
    "concentrated-liquidity",
    "api",
    "rest-api"
  ],
  "author": "Midcurve Finance",
  "license": "MIT",
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@midcurve/services": "file:.yalc/@midcurve/services",
    "@midcurve/shared": "file:.yalc/@midcurve/shared",
    "@prisma/client": "^6.17.1",
    "nanoid": "^5.1.6",
    "next": "^15.5.0",
    "next-auth": "^5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "siwe": "^2.3.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.5.0",
    "prisma": "^6.17.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
